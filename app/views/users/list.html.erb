<h1>List of Users</h1>

<%= will_paginate @users %>

<table class="table table-striped table-bordered">
<tr>
	<th>Email</th>
	<th>Member Since</th>
</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= link_to user.email, user_path(user) %></td>
		<td><%= user.created_at.strftime "%B %Y" %></td>
	</tr>
<% end %>
</table>